import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine().toLowerCase();
        String str1 = str.replaceAll("[aeiou]","");
        System.out.println(str1);
        
    }
}

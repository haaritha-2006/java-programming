import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        char ch=s.next().charAt(0);
        float add=a+b;
        float sub=a-b;
        float mul=a*b;
        float div=a/b;
        float mod=a%b;
        switch(ch){
            case '+':
                System.out.printf("Addition of two number is %.1f",add);
                break;
            case '-':
                System.out.printf("Subtraction of two number is %.1f",sub);
                break;
             case '*':
                System.out.printf("Multiplication of two number is %.1f",mul);
                break;   
            case '/':
                System.out.printf("Division of two number is %.1f",div);
                break;
            case '%':
                System.out.printf("Modulo of two number is %.1f",mod);
                break;
            default:
                System.out.println("Invalid Input");
        }
    }
}

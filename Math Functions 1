import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        float num=s.nextFloat();
        int num1=s.nextInt();
        int num2=s.nextInt();
        int num3=s.nextInt();
        int floorvalue=(int)Math.floor(num);
        int ceilvalue=(int)Math.ceil(num);
        int sqrtvalue=(int)Math.sqrt(num1);
        int powvalue=(int)Math.pow(num2,num3);
        System.out.println(floorvalue);
        System.out.println(ceilvalue);
        System.out.println(sqrtvalue);
        System.out.println(powvalue);
        
    }
}

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int num1=s.nextInt();
        float num2=s.nextFloat();
        System.out.println(num1);
        System.out.printf("%.2f",num2);
    }
}
